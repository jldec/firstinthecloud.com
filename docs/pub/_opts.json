},{"path":"/editor-upload.hbs","text":"<!DOCTYPE html>\n<html>\n<head>\n<meta charset=\"UTF-8\">\n<title>pub editor - {{title}}</title>\n<meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n<link rel=\"stylesheet\" href=\"{{relPath}}/css/normalize.css\">\n<link rel=\"stylesheet\" href=\"{{relPath}}/css/editor.css\">\n</head>\n<body>\n<div class=\"upload\">\n<h3>{{title}}</h3>\n<form method=\"POST\" enctype=\"multipart/form-data\">\n{{{html}}}\n</form>\n{{#eachUpload}}<pre>![](/images/{{{title}}}){{s3Err}}\n</pre>\n<img src=\"{{option 'fqImages'}}/images/{{title}}\"><br>\n{{option 'fqImages'}}/images/{{title}}\n{{/eachUpload}}\n</div>\n<script src=\"{{relPath}}/pub/js/jquery-1.12.4.min.js\"></script>\n</body>\n</html>\n"}]}],"staticPaths":[{"path":"/Users/jleschner/pub/firstinthecloud/images","route":"/images","watch":{},"name":"staticPath:/Users/jleschner/pub/firstinthecloud/images","depth":5,"maxAge":"10m","includeBinaries":true,"sendOpts":{"maxAge":"10m","dotfiles":"ignore","index":false,"extensions":false,"root":"/Users/jleschner/pub/firstinthecloud/images"}},{"path":"/Users/jleschner/pub/firstinthecloud/README.md","watch":{},"route":"/","name":"staticPath:/Users/jleschner/pub/firstinthecloud/README.md","depth":5,"maxAge":"10m","includeBinaries":true,"sendOpts":{"maxAge":"10m","dotfiles":"ignore","index":false,"extensions":false,"root":"/Users/jleschner/pub/firstinthecloud"}},{"path":"/Users/jleschner/pub/firstinthecloud/LICENSE","watch":{},"route":"/","name":"staticPath:/Users/jleschner/pub/firstinthecloud/LICENSE","depth":5,"maxAge":"10m","includeBinaries":true,"sendOpts":{"maxAge":"10m","dotfiles":"ignore","index":false,"extensions":false,"root":"/Users/jleschner/pub/firstinthecloud"}},{"path":"/Users/jleschner/pub/firstinthecloud/CNAME","watch":{},"route":"/","name":"staticPath:/Users/jleschner/pub/firstinthecloud/CNAME","depth":5,"maxAge":"10m","includeBinaries":true,"sendOpts":{"maxAge":"10m","dotfiles":"ignore","index":false,"extensi